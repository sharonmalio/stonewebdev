{
	"name" : "zendframework/skeleton-application",
	"description" : "Skeleton Application for Zend Framework zend-mvc applications",
	"type" : "project",
	"license" : "BSD-3-Clause",
	"keywords" : [
		"framework",
		"mvc",
		"zf"
	],
	"homepage" : "http://framework.zend.com/",
	"minimum-stability" : "dev",
	"prefer-stable" : true,
	"require" : {
		"php" : "^5.6 || ^7.0",
		"zendframework/zend-component-installer" : "^1.0 || ^0.7 || ^1.0.0-dev@dev",
		"zendframework/zend-mvc" : "^3.0.1",
		"zfcampus/zf-development-mode" : "^3.0",
		"zendframework/zend-db" : "^2.9",
		"zendframework/zend-hydrator" : "^2.4",
		"zendframework/zend-servicemanager" : "^3.3.1",
		"zendframework/zend-inputfilter" : "^2.8",
		"zendframework/zend-form" : "^2.12",
		"zendframework/zend-router" : "^3.0",
		"zendframework/zend-i18n" : "^2.9",
		"zf-commons/zfc-user" : "^3.0",
		"zf-commons/zfc-user-doctrine-orm": "dev-master",
		"zendframework/zend-authentication" : "^2.6",
		"zendframework/zend-expressive-authentication-session" : "^0.5.0",
		"zendframework/zend-dom": "^2.7",
		"zendframework/zend-debug": "^2.6"
	},
	"autoload" : {
		"psr-4" : {
			"Application\\" : "module/Application/src/",
			"Stonelink\\" : "module/Stonelink/src/",
			"Stonechat\\" : "module/Stonechat/src/"
		}
	},
	"autoload-dev" : {
		"psr-4" : {
			"ApplicationTest\\" : "module/Application/test/",
			"Stonelink\\" : "module/Stonelink/src/",
			"Stonechat\\" : "module/Stonechat/src/"
		}
	},
	"scripts" : {
		"cs-check" : "phpcs",
		"cs-fix" : "phpcbf",
		"development-disable" : "zf-development-mode disable",
		"development-enable" : "zf-development-mode enable",
		"development-status" : "zf-development-mode status",
		"post-create-project-cmd" : "@development-enable",
		"serve" : "php -S 0.0.0.0:8080 -t public public/index.php",
		"test" : "phpunit"
	}
}

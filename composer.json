{
	"name" : "zendframework/skeleton-application",
	"description" : "Skeleton Application for Zend Framework zend-mvc applications",
	"type" : "project",
	"license" : "BSD-3-Clause",
	"keywords" : [
		"framework",
		"mvc",
		"zf"
	],
	"homepage" : "http://framework.zend.com/",
	"minimum-stability" : "dev",
	"prefer-stable" : true,
	"require" : {
		"php" : "^5.6 || ^7.0",
		"zendframework/zend-component-installer" : "^1.0 || ^0.7 || ^1.0.0-dev@dev",
		"zendframework/zend-mvc" : "^3.0.1",
		"zfcampus/zf-development-mode" : "^3.0",
		"zendframework/zend-cache" : "^2.7.1",
		"zendframework/zend-db" : "^2.8.1",
		"zendframework/zend-mvc-form" : "^1.0",
		"zendframework/zend-json" : "^3.1",
		"zendframework/zend-log" : "^2.9",
		"zendframework/zend-mvc-console" : "^1.1.10",
		"zendframework/zend-mvc-i18n" : "^1.0",
		"zendframework/zend-mvc-plugins" : "^1.0",
		"zendframework/zend-psr7bridge" : "^0.2.2",
		"zendframework/zend-session" : "^2.7.1",
		"zendframework/zend-servicemanager-di" : "^1.0",
		"zf-commons/zfc-user" : "^3.0",
		"zendframework/zend-i18n-resources" : "^2.6",
		"zendframework/zend-mvc-plugin-flashmessenger" : "^1.1",
		"zendframework/zend-mvc-plugin-fileprg" : "^1.1",
		"zendframework/zend-mvc-plugin-identity" : "^1.1",
		"zendframework/zend-debug" : "^2.6",
		"doctrine/doctrine-orm-module" : "^2.1",
		"zendframework/zend-authentication" : "^2.6",
		"zendframework/zend-captcha" : "^2.8",
		"zendframework/zend-permissions-acl" : "^2.7",
		"league/oauth2-server" : "^7.3",
		"zfcampus/zf-oauth2" : "^1.5",
		"doctrine/dbal": "^2.9",
		"zendframework/zend-mail": "^2.10",
		"africastalking/africastalking": "^2.1"
	},
	"autoload" : {
		"psr-4" : {
            "Application\\": "module/Application/src/",
            "Appointments\\": "module/Appointments/src/",
            "Stonebase\\": "module/Stonebase/src/",
            "Stonelink\\": "module/Stonelink/src/",
            "Users\\": "module/Users/src/"
		}
	},
	"autoload-dev" : {
		"psr-4" : {
            "ApplicationTest\\": "module/Application/test/",
            "AppointmentsTest\\": "module/Appointments/test/",
            "StonebaseTest\\": "module/Stonebase/test/",
            "StonelinkTest\\": "module/Stonelink/test/",
            "UsersTest\\": "module/Users/test/"
		}
	},
	"scripts" : {
		"cs-check" : "phpcs",
		"cs-fix" : "phpcbf",
		"development-disable" : "zf-development-mode disable",
		"development-enable" : "zf-development-mode enable",
		"development-status" : "zf-development-mode status",
		"post-create-project-cmd" : "@development-enable",
		"serve" : "php -S 0.0.0.0:8080 -t public public/index.php",
		"test" : "phpunit"
	},
	"require-dev" : {
		"zendframework/zend-test" : "^3.2"
	}
}
